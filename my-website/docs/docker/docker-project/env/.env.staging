# Staging Environment Configuration
APP_NAME=MyApp
APP_ENV=staging
DEBUG=false

# Database Configuration
DB_PASSWORD=staging_secure_password_123
DB_HOST=postgres
DB_PORT=5432
DB_NAME=myapp
DB_USER=appuser

# Redis Configuration
REDIS_PASSWORD=staging_redis_password_456
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0

# Security
JWT_SECRET_KEY=staging_jwt_secret_key_make_it_long_and_secure
SECRET_KEY=staging_flask_secret_key_also_secure

# API Configuration
API_HOST=0.0.0.0
API_PORT=8080
API_WORKERS=2

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Email Configuration
MAIL_SERVER=smtp.staging.com
MAIL_PORT=587
MAIL_USE_TLS=true
MAIL_USERNAME=staging@example.com
MAIL_PASSWORD=staging_mail_password

# External Services
SENTRY_DSN=https://staging_sentry_dsn_here

# File Upload
MAX_CONTENT_LENGTH=16777216
UPLOAD_FOLDER=/app/uploads

# Cache Configuration
CACHE_TYPE=redis
CACHE_DEFAULT_TIMEOUT=300

# SSL
SSL_DISABLE=false
